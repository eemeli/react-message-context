{
  "name": "react-message-context",
  "version": "0.4.0",
  "description": "React message provider, using the Context API",
  "repository": "eemeli/react-message-context",
  "keywords": [
    "i18n",
    "React",
    "context",
    "messages",
    "provider"
  ],
  "author": "Eemeli Aro <eemeli@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/eemeli/react-message-context#readme",
  "main": "dist/index.js",
  "files": [
    "API.md",
    "dist/"
  ],
  "scripts": {
    "build": "babel src -d dist",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "jest": {
    "testMatch": [
      "**/tests/*.js"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "immutable": "^3.8.2",
    "jest": "^24.8.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6"
  },
  "peerDependencies": {
    "prop-types": ">=15.5.7",
    "react": ">=16.3.0"
  }
}
